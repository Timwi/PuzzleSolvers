<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <OutputPath>..\Builds\$(Configuration)-$(TargetFramework)$(RuntimeIdentifier)\</OutputPath>
    <IntermediateOutputPath>$(BaseIntermediateOutputPath)\$(Configuration)-$(TargetFramework)$(RuntimeIdentifier)\</IntermediateOutputPath>
    <DocumentationFile>$(OutputPath)\$(AssemblyName).xml</DocumentationFile>
    <Configurations>Debug;Release</Configurations>
    <Version>1.0.0.9999</Version>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Properties\**" />
    <Compile Remove="RT.Util.Core\**" />
    <EmbeddedResource Remove="Properties\**" />
    <EmbeddedResource Remove="RT.Util.Core\**" />
    <None Remove="Properties\**" />
    <None Remove="RT.Util.Core\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Func.cs" />
    <Compile Remove="PuzzleNETFramework.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="RT.Util.Core" Version="2.0.1684" />
  </ItemGroup>

</Project>
